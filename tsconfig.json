{
  "compilerOptions": {
    "declaration": false,
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "sourceMap": true,
    //"strictNullChecks": true,
    "lib": ["es2017", "dom"]
  },
  "compileOnSave": true,
  "exclude": [
    "build",
    "node_modules/*",
    "bower_components/**/demo/*",
    "bower_components/**/test/*"
  ]
}